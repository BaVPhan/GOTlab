<h1>ADD A NEW HOUSE!</h1>


<%= form_for @house do |f| %>
  <p>
      <%= f.label :name %><br>
      <%= f.text_field :name %>
    </p>

    <p>
      <%= f.label :picture %><br>
      <%= f.text_area :img_url%>
    </p>

    <p>
      <%= f.label :region %><br>
      <%= f.text_area :region%>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>
